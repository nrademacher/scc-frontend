{
  "name": "scc-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn type-check && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|js|css|json)\" --write --list-different --config prettier.config.js",
    "type-check": "tsc",
    "start:test": "start-server-and-test 'yarn start' http://localhost:3000",
    "test": "yarn start:test 'cypress open'",
    "test:headless": "yarn start:test 'cypress run'"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "deepmerge": "^4.2.2",
    "graphql": "^16.2.0",
    "js-cookie": "^3.0.1",
    "lodash.isequal": "^4.5.0",
    "next": "^12.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "subscriptions-transport-ws": "^0.11.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.2",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.8",
    "@types/react": "^18.0.8",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.0",
    "cypress": "^9.2.0",
    "eslint": "^8.5.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
